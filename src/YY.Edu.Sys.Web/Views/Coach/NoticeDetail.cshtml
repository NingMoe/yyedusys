                    <i class="am-header-icon am-icon-arrow-left"></i>
                </a>
            </div>
            <h1 class="am-header-title">
                <a href="javascript:void(0);" class="">详细信息</a>
            </h1>
            <div class="am-header-right am-header-nav">
                <a href='@Url.Action("Index","Coach")' class=""><i class="icon am-icon-home iconfont textAlign"></i></a>
            </div>
        </header>

        <!-- 内容部分 -->
        <div class="_remarkCon">
            <div data-am-widget="tabs" class="am-tabs am-tabs-default">

                <div>

                    <div data-tab-panel-1 class="am-tab-panel ">
                        <p class="_rem_stroy "></p>
                        <!-- 阶段总结 -->
                        <div class="_CD_NOTsc">

                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
    <input type="hidden" id="hdNoticeID" name="hdNoticeID" value="@ViewBag.DetailID" />

    @*<script src="/Scripts/amazeui.min.js"></script>*@
    <script src="/Scripts/app.js"></script>
    <script src="/Scripts/Notice/Detail.js"></script>
