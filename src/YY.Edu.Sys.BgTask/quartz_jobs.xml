<?xml version="1.0" encoding="utf-8" ?>
<job-scheduling-data xmlns="http://quartznet.sourceforge.net/JobSchedulingData">
  <processing-directives>
    <overwrite-existing-data>true</overwrite-existing-data>
  </processing-directives>

  <schedule>
    
    <!--百度热点-->
    <job>
      <name>BaiduHotJob</name>
      <group>HotJob</group>
      <description>BaiduHotJob</description>
      <job-type>QZJP.WXCrawler.ClientUI.BaiduHotJob, QZJP.WXCrawler.ClientUI</job-type>
      <durable>true</durable>
      <recover>false</recover>
    </job>
    <trigger>
      <cron>
        <name>BaiduHotTrigger</name>
        <group>HotJobSimpleGroup</group>
        <job-name>BaiduHotJob</job-name>
        <job-group>HotJob</job-group>
        <cron-expression>2 1 10 * * ? </cron-expression>
      </cron>
    </trigger>

